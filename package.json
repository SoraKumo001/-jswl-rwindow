    "@storybook/addon-viewport": "^6.1.11",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.11",
    "@types/node": "^14.14.20",
    "@types/react": "~17.0.0",
    "@types/react-dom": "~17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
